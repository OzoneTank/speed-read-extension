{
    "name": "speed-read",
    "version": "0.0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "speed-read.html",
        "default_title": "Speed Read"
    }
}